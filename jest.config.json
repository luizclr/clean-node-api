{
  "roots": ["<rootDir>/src"],
  "collectCoverageFrom": ["<rootDir>/src/**/*.ts"],
  "coveragePathIgnorePatterns": ["<rootDir>/src/index.ts"],
  "coverageDirectory": "coverage",
  "collectCoverage": true,
  "coverageProvider": "v8",
  "testEnvironment": "node",
  "transform": {
    ".+\\.ts$": "ts-jest"
  }
}
