{
  "name": "clean-node-api",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "prepare": "husky install",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "rm -rf dist && tsc",
    "lint": "eslint 'src/**/*.ts'",
    "lint:fix": "yarn lint --fix",
    "lint:staged": "lint-staged",
    "test": "jest --no-cache --passWithNoTests --noStackTrace",
    "test:unit": "yarn test --watch -c jest-unit.config.ts",
    "test:integration": "yarn test --watch -c jest-integration.config.ts --runInBand",
    "test:watch": "yarn test --watch",
    "test:coverage": "yarn test --coverage",
    "test:coveralls": "yarn test:coverage && coveralls < coverage/lcov.info"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "express": "^4.18.1",
    "http-status-codes": "^2.2.0",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.2.0",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.5.1",
    "@types/node": "^17.0.35",
    "@types/supertest": "^2.0.12",
    "@types/validator": "^13.7.3",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^5.26.0",
    "coveralls": "^3.1.1",
    "eslint": "^7.12.1",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.0.0",
    "git-commit-msg-linter": "^4.1.3",
    "husky": "^8.0.1",
    "jest": "^28.1.0",
    "lint-staged": "^12.4.1",
    "supertest": "^6.2.3",
    "ts-jest": "^28.0.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4"
  },
  "engines": {
    "node": ">=16.0"
  }
}
